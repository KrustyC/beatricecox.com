{
  "name": "beatricecox.com",
  "private": false,
  "scripts": {
    "dev": "npm run prisma:generate && next dev --turbo",
    "build": "npm run prisma:generate && npm run prisma:migrate:deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate-dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.10.3",
    "@apollo/experimental-nextjs-app-support": "^0.10.1",
    "@contentful/rich-text-plain-text-renderer": "^16.0.13",
    "@contentful/rich-text-react-renderer": "^15.19.6",
    "@next/third-parties": "^14.2.3",
    "@prisma/client": "^5.13.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "classnames": "^2.5.1",
    "contentful": "^10.10.0",
    "framer-motion": "^11.1.9",
    "next": "^14.2.3",
    "prisma": "^5.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-responsive-carousel": "^3.2.23",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/typescript": "^4.0.6",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
