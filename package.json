{
  "name": "beatricecox.com",
  "private": false,
  "scripts": {
    "dev": "npm run prisma:generate && next dev --turbopack",
    "build": "npm run prisma:generate && npm run prisma:migrate:deploy && next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check src",
    "format": "prettier --write src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate-dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma"
  },
  "engines": {
    "npm": ">=10.9.3",
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@next/third-parties": "16.1.3",
    "@portabletext/react": "^6.0.2",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sanity/client": "^7.14.0",
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.22.0",
    "@uidotdev/usehooks": "2.4.1",
    "@vercel/analytics": "1.6.1",
    "@vercel/speed-insights": "1.3.1",
    "classnames": "2.5.1",
    "framer-motion": "12.26.2",
    "next": "16.1.3",
    "next-sanity": "^11.6.12",
    "pg": "^8.13.1",
    "prisma": "7.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-responsive-carousel": "3.2.23",
    "sanity": "^4.22.0",
    "schema-dts": "1.1.5",
    "server-only": "0.0.1",
    "styled-components": "^6.3.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "22.15.17",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "postcss": "8.5.6",
    "prettier": "3.8.0",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
