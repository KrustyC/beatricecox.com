{
  "name": "beatricecox.com",
  "private": false,
  "scripts": {
    "dev": "npm run prisma:generate && next dev --turbo",
    "build": "npm run prisma:generate && npm run prisma:migrate:deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate-dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.9.0",
    "@apollo/experimental-nextjs-app-support": "^0.8.0",
    "@contentful/rich-text-plain-text-renderer": "^16.0.11",
    "@contentful/rich-text-react-renderer": "^15.19.4",
    "@next/third-parties": "^14.1.0",
    "@prisma/client": "^5.9.1",
    "@react-spring/web": "^9.7.3",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.9",
    "contentful": "^10.6.21",
    "next": "^14.1.0",
    "prisma": "^5.9.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@types/node": "^20.11.13",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
