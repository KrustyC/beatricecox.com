{
  "name": "beatricecox.com",
  "private": false,
  "scripts": {
    "dev": "npm run prisma:generate && next dev --turbo",
    "build": "npm run prisma:generate && npm run prisma:migrate:deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check src",
    "format": "prettier --write src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate-dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@contentful/rich-text-plain-text-renderer": "^17.0.0",
    "@contentful/rich-text-react-renderer": "^16.0.1",
    "@headlessui/react": "^2.2.2",
    "@next/third-parties": "^15.3.1",
    "@prisma/client": "^6.6.0",
    "@uidotdev/usehooks": "^2.4.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "classnames": "^2.5.1",
    "contentful": "^11.5.16",
    "framer-motion": "^12.9.2",
    "i": "^0.3.7",
    "next": "^14.2.5",
    "npm": "^11.3.0",
    "prisma": "^6.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-responsive-carousel": "^3.2.23",
    "schema-dts": "^1.1.5",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@types/node": "^22.15.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.8.3"
  }
}
